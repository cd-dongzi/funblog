{
  "name": "client",
  "version": "1.1.10",
  "private": true,
  "scripts": {
    "dev": "next dev --port $CLIENT_PORT",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev --port $CLIENT_PORT",
    "build": "next build",
    "build:preview": "ANALYZE=true next build",
    "start": "next start --port $CLIENT_PORT",
    "start:pm2": "pm2 start pnpm --name client -- start",
    "stop:pm2": "pm2 stop client",
    "delete:pm2": "pm2 delete client",
    "lint": "next lint",
    "lint:fix": "eslint --fix --quiet 'src/**/*.{ts,tsx}'",
    "replace:workspace": "funblog replace workspace"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "iOS >= 7",
    "Android >= 4.1",
    "not ie <= 8"
  ],
  "dependencies": {
    "@funblog/components": "workspace:*",
    "@funblog/constants": "workspace:*",
    "@funblog/hooks": "workspace:*",
    "@funblog/utils": "workspace:*",
    "@popperjs/core": "^2.11.8",
    "classnames": "^2.3.2",
    "colorthief": "^2.4.0",
    "core-js": "3",
    "cross-env": "^7.0.3",
    "framer-motion": "10.16.4",
    "highlight.js": "^11.9.0",
    "lodash": "^4.17.21",
    "marked": "^10.0.0",
    "next": "14.2.0-canary.11",
    "pm2": "^5.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "^18.2.0",
    "react-popper": "^2.3.0",
    "swc-plugin-another-transform-imports": "^0.2.7",
    "swc-plugin-transform-import": "^1.2.0"
  },
  "devDependencies": {
    "@funblog/cli": "workspace:*",
    "@funblog/eslint-config": "workspace:*",
    "@funblog/tsconfig": "workspace:*",
    "@funblog/types": "workspace:*",
    "@next/bundle-analyzer": "^14.0.3",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "@types/react-is": "^18.2.4",
    "autoprefixer": "^10.4.16",
    "babel-plugin-import": "^1.13.8",
    "postcss": "^8",
    "postcss-import": "^15.1.0",
    "postcss-mixins": "^9.0.4",
    "tailwindcss": "^3",
    "terser-webpack-plugin": "^5.3.9"
  }
}
